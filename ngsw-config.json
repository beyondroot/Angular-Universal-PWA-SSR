{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "freshness",
      "urls": [
        "/",
        "/fruits",
        "/animals"
      ],
      "cacheConfig": {
        "maxSize": 100,
        "maxAge": "1d",
        "timeout": "5s",
        "strategy": "freshness"
      }
    },
    {
      "name": "performance",
      "urls": [
        "/fruit/*",
        "/animal/*"
      ],
      "cacheConfig": {
        "maxSize": 100,
        "maxAge": "7d",
        "timeout": "5s",
        "strategy": "performance"
      }
    }
  ]
}